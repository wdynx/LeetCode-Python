class Solution:
    def merge(self, intervals: List[List[int]]) -> List[List[int]]:
        intervals.sort(key=lambda x:x[0])
        res=[]
        for interval in intervals:
            if res!=[] and interval[0]<=res[-1][1]:
                res[-1][1]=max(interval[1],res[-1][1])
            else:
                res.append(interval)
        return res
