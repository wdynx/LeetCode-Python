# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution:
    def removeElements(self, head: ListNode, val: int) -> ListNode:
        while head and head.val==val:
            head=head.next
        if not head:
            return None
        p=head
        while p.next:
            if p.next.val==val:
                p.next=p.next.next
            else:
                p=p.next
        return head
