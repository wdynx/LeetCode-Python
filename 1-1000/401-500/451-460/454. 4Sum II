class Solution:
    def fourSumCount(self, A: List[int], B: List[int], C: List[int], D: List[int]) -> int:
        dic = {}
        for i in A:
            for j in B:
                dic[i+j] = dic.get(i+j, 0) + 1
        res = 0
        for i in C:
            for j in D:
                res += dic.get(-i-j, 0)
        return res
